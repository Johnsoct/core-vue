        "rollup-plugin-copy": "^3.5.0",
        "sass": "^1.70.0",
        "start-server-and-test": "^2.0.3",
        "stylelint": "^16.18.0",
        "stylelint-config-standard": "^38.0.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "typescript": "^5.3.3",
        "typescript-eslint": "^8.30.1",
        "vite": "^5.0.10",
        "vitest": "^1.0.4",
        "vue-tsc": "^1.8.27"
